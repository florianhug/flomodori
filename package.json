{
  "name": "flomodori",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run copy:readme && npm run copy:license",
    "test": "ng test flomodori --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy:readme": "copyfiles ./README.md ./dist/flomodori",
    "copy:license": "copyfiles ./LICENSE ./dist/flomodori",
    "publish:showcase": "npx angular-cli-ghpages --dir=./dist/flomodori",
    "build:showcase": "ng build flomodori --configuration production --base-href='https://florianhug.github.io/flomodori/'",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.8",
    "@angular/common": "^12.2.8",
    "@angular/compiler": "^12.2.8",
    "@angular/core": "^12.2.8",
    "@angular/forms": "^12.2.8",
    "@angular/platform-browser": "^12.2.8",
    "@angular/platform-browser-dynamic": "^12.2.8",
    "@angular/pwa": "^0.901.8",
    "@angular/router": "^12.2.8",
    "@angular/service-worker": "^12.2.8",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.8",
    "@angular/cli": "^12.2.8",
    "@angular/compiler-cli": "^12.2.8",
    "@angular/language-service": "^12.2.8",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.13",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
